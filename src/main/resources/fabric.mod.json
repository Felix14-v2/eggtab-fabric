{
  "schemaVersion": 1,
  "id": "eggtab",
  "version": "${version}",

  "name": "Egg Tab",
  "description": "Moves all spawn eggs and enchanted books to separate creative tabs.",
  "authors": ["Florens Pauwels"],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/eggtab",
    "sources": "https://github.com/florensie/eggtab",
    "issues": "https://github.com/florensie/eggtab/issues"
  },

  "license": "MIT",
  "icon": "assets/eggtab/icon.png",

  "entrypoints": {
    "client": ["be.florens.eggtab.EggTab"],
    "modmenu": ["be.florens.eggtab.config.ModMenuConfig"]
  },

  "mixins": [
    "eggtab.mixins.json"
  ],

  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "modmenu": "*"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
